{
  "defaultSeverity": "error",
  "extends": [
    "tslint:all",
    "tslint-react"
  ],
  "rules": {
    "typedef": false,
    "prefer-function-over-method": false,
    "completed-docs": false,
    "no-unsafe-any": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules",
      "*.js"
    ]
  }
}
